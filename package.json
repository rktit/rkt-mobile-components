{
  "name": "rocket_mobile_components",
  "description": "Rocket Mobile Components is a library of reusable components for React Native applications.",
  "version": "0.1.8",
  "private": false,
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "prepare": "bob build",
    "release": "release-it --no-git.requireUpstream"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.4.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^7.2.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "i18next": "^23.10.1",
    "react": "18.2.0",
    "react-hook-form": "^7.51.4",
    "react-i18next": "^14.1.0",
    "react-native": "0.73.5",
    "react-native-config": "^1.5.1",
    "react-native-mask-input": "^1.2.3",
    "react-native-reanimated": "^3.11.0",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^15.2.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-query": "^3.39.3",
    "styled-components": "^6.1.8",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@release-it/conventional-changelog": "^8.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.3.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.6",
    "@types/react": "^18.2.30",
    "@types/react-native": "^0.72.5",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.34.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-native-builder-bob": "^0.23.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/rktit/rkt-mobile-componentts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release :rocket: v${version}",
      "tagName": "v${version}",
      "requireUpstream": false,
      "requireCleanWorkingDir": false
    },
    "npm": {
      "publish": true,
      "private": false
    }
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": {
        "name": "angular"
      },
      "infile": "CHANGELOG.md"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation/(.*))"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "rpc1910-ui.podspec",
    "react-native.config.js",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "src/**/*.ts": [
    "tsc-files --noEmit"
  ]
}
